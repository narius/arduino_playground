/**/const int ledPinGreen = 2;const int ledPinYellow = 3;int ledState = LOW;int pin = ledPinGreen;void setup() {  pinMode(ledPinGreen, OUTPUT);  pinMode(ledPinYellow, OUTPUT);  digitalWrite(ledPinGreen, LOW);  digitalWrite(ledPinYellow, LOW);}void loop() {  if(pin==ledPinGreen){    pin=ledPinYellow;    digitalWrite(ledPinGreen, LOW);  }  else {    pin=ledPinGreen;    digitalWrite(ledPinYellow, LOW);  }  digitalWrite(pin, HIGH);  delay(500);}