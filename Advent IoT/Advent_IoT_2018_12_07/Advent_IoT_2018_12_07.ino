/**/int analogPin = A5;int analogValue;const int ledPinGreen = 2;const int ledPinYellow = 3;const int ledPinBlue = 4;int ledState = LOW;int pin = ledPinGreen;void setup() {  pinMode(analogPin, INPUT);  pinMode(ledPinGreen, OUTPUT);  pinMode(ledPinYellow, OUTPUT);  pinMode(ledPinBlue, OUTPUT);  digitalWrite(ledPinGreen, LOW);  digitalWrite(ledPinYellow, LOW);  digitalWrite(ledPinBlue, LOW);  Serial.begin(9600);}void loop() {  analogValue = analogRead(analogPin); Serial.println(analogValue); delay(analogValue); digitalWrite(ledPinGreen, LOW); digitalWrite(ledPinBlue, HIGH ); delay(analogValue); digitalWrite(ledPinBlue, LOW); digitalWrite(ledPinYellow, HIGH); delay(analogValue); digitalWrite(ledPinYellow, LOW); digitalWrite(ledPinGreen, HIGH);}